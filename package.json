{
  "name": "-web",
  "description": "web front end with VueJs",
  "version": "1.0.0",
  "author": "",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build-dev": "cross-env NODE_ENV=dev npm run clean && npm run compile-dev && rm -rf dist && mv dist2 dist",
    "build-test": "cross-env NODE_ENV=test npm run clean && npm run compile-test",
    "build:parts": "npm run compile",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist2 && rimraf coverage",
    "compile-dev": "webpack --env dev --config config/webpack.config.prod.js",
    "compile-test": "webpack --env test --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "dev": "webpack-dev-server --env dev --hot --inline",
    "dev:test": "webpack-dev-server --env test --hot --inline",
    "lint": "tslint src/**/*.ts",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch",
    "deploy-dev": "npm run build-dev && firebase deploy -P dev",
    "deploy-test": "npm run build-test && firebase deploy -P test"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.2.0",
