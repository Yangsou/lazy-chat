    "@hgiasac/helper": "^0.1.9",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "~5.2.5",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.2",
    "axios": "^0.18.1",
    "element-ui": "2.4.9",
